<?xml version="1.0"?>
<launch>

    <arg name="gui" default="false"/>
    <arg name="gazebo_model_name" default="robot"/>
    <arg name="start_gazebo" default="true"/>
    <arg name="gazebo_world" default="worlds/empty.world"/>
    <arg name="use_cam_tool" default="false" />
    
    <include file="$(find ur_gazebo)/launch/ur3_bringup.launch">
     <arg name="gui" value="$(arg gui)"/>
     <arg name="gazebo_model_name" value="$(arg gazebo_model_name)"/>
     <arg name="start_gazebo" value="$(arg start_gazebo)"/>
     <arg name="gazebo_world" value="$(arg gazebo_world)"/>
     <arg name="use_cam_tool" value="$(arg use_cam_tool)" />
    </include> 

    <include file="$(find ur3_moveit_config)/launch/moveit_planning_execution.launch">
      <arg name="sim" value="true" />
    </include> 

  <include file="$(find timed_roslaunch)/launch/timed_roslaunch.launch">
    <arg name="time" value="2" />
    <arg name="pkg" value="dmp" />
    <arg name="file" value="dmp.launch" />
  </include>

  <!-- <include file="$(find timed_roslaunch)/launch/timed_roslaunch.launch">
    <arg name="time" value="4" />
    <arg name="pkg" value="ur3_moveit_config" />
    <arg name="file" value="ur3_moveit_planning_execution.launch" />
  </include> -->


</launch>
